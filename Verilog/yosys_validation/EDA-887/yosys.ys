 
# Yosys synthesis script for ram_true_dp_wf_1024x32
# Read source files
read -sv ${ROOT_PATH}/yosys/install/share/yosys/rapidsilicon/${ARCHITECTURE}/cell_sim_blackbox.v
verific -vlog-incdir ${ROOT_PATH}/RTL_Benchmark/Verilog/yosys_validation/EDA-887/
verific -vlog-libdir ${ROOT_PATH}/RTL_Benchmark/Verilog/yosys_validation/EDA-887/
verific -vlog-libext .v
verific -vlog-libext .sv
verific -vlog-define 
verific -vlog2k ${ROOT_PATH}/RTL_Benchmark/Verilog/yosys_validation/EDA-887/ram_true_dp_wf_1024x32.sv
verific -import ram_true_dp_wf_1024x32


# Technology mapping
hierarchy -top ram_true_dp_wf_1024x32



plugin -i synth-rs

synth_rs  -tech ${ARCHITECTURE} -top ram_true_dp_wf_1024x32 -de -goal delay -carry no  -de_max_threads -1   

write_verilog -noattr -nohex ram_true_dp_wf_1024x32_post_synth.v
