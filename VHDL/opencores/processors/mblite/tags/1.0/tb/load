restart
mem load -infile rom.mem      -format hex /imem/ram
mem load -infile ram.mem      -format hex /dmem/ram
mem load -infile reg_file.mem -format hex /core0/decode0/a/ram
mem load -infile reg_file.mem -format hex /core0/decode0/b/ram
mem load -infile reg_file.mem -format hex /core0/decode0/d/ram
run 100ns