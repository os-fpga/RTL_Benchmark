create_clock  -name {CLK} {p:CLK} -period {20}
create_clock  -name {clk_wishbone_inst} {p:clk_wishbone_inst} -period {20}
create_clock  -name {clk_core} {p:clk_core} -period {12.62}
create_clock  -name {clk_wishbone_data} {p:clk_wishbone_data} -period {20}
set_clock_groups -asynchronous -group [get_clocks {CLK}] -group [get_clocks {clk_wishbone_inst}] -group [get_clocks {clk_core}] -group [get_clocks {clk_wishbone_data}]

